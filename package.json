{
  "name": "minji",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --mode development",
    "build": "npm run cleanbuild && NODE_ENV=production webpack --mode production && npm run copyhtaccess",
    "cleanbuild": "rm -rf ./dist",
    "copyhtaccess": "cp ./src/.htaccess ./dist",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{js,vue}; exit 0",
    "eslint:fix": "./node_modules/.bin/eslint ./src/**/*.{js,vue} --fix; exit 0",
    "stylelint": "./node_modules/.bin/stylelint ./src/**/*.{scss,css,sass,vue}; exit 0",
    "stylelint:fix": "./node_modules/.bin/stylelint ./src/**/*.{scss,css,sass,vue} --fix; exit 0",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix"
  },
  "browserslist": [
    "ie 10",
    "defaults"
  ],
  "author": "hugosocie",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "engines": {
    "node": "5.7.0",
    "npm": "3.6.0"
  },
  "dependencies": {
    "reset-css": "^4.0.1",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  }
}
